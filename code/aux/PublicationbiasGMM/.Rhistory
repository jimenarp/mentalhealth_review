p.stars <-function(pval, se) {
if (pval<=0.001) {
se<-paste(se, '***',sep='' )
} else if  (pval>0.001 & pval<=0.05) {
se<-paste(se, '**',sep='' )
} else if  (pval>0.05 & pval<=0.1) {
se<-paste(se, '*',sep='' )
} else {
se<-se
}
}
toformat <-function(number, n) {
number=as.character(prettyNum(round(number,n),nsmall=n, big.mark=',', format=f))
}
